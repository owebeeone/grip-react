{
  "name": "@owebeeone/grip-react",
  "version": "0.2.0",
  "description": "React integration for GRIP (Generalized Retrieval Intent Provisioning) - A reactive dataflow library for unified state management",
  "main": "./dist/index.js",
  "scripts": {
    "test": "vitest run --coverage",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "react",
    "state-management",
    "reactive",
    "dataflow",
    "grip",
    "context-graph"
  ],
  "author": "Gianni Mariani",
  "license": "MIT",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@owebeeone/grip-core": "^0.2.1"
  }
}
